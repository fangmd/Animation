<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.waytone.animationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".AnimationOneActivity"
            android:label="@string/title_activity_animation_one"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".transition.TransitionActivity">
        </activity>
        <activity android:name=".transition.TransitionExplodeActivity">
        </activity>
        <activity android:name=".VectorAct">
        </activity>
        <activity android:name=".textv.TextViewAct">
        </activity>
        <activity android:name=".indicate.IndicateAct">
        </activity>
    </application>

</manifest>